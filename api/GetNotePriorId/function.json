{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "priornoteid/{blog}/{ts}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "notes",
      "databaseName": "kotaro",
      "collectionName": "notes",
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "in",
      "SqlQuery": "select TOP 1 r.id from r where r.blogid = {blog} and r._ts < StringToNumber({ts}) order by r._ts DESC"
    }
  ]
}

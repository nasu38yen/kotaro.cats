{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "notecount/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "values",
      "databaseName": "kotaro",
      "collectionName": "notes",
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "in",
      "PartitionKey": "1",
      "SqlQuery": "select count(r.id) from notes r where r.blogid = {id}"
    }
  ]
}
